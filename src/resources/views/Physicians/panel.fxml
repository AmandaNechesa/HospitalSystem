<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="panel" prefHeight="700.0" prefWidth="1200.0" styleClass="basicscene"
            stylesheets="@../css/basicscene.css" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Physicians.PanelController">
    <children>
        <StackPane layoutY="66.0" prefHeight="633.0" prefWidth="1200.0" stylesheets="@../css/basicscene.css">
            <children>
                <TabPane fx:id="tabContainer" prefHeight="623.0" prefWidth="1200.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab fx:id="searchPatientTab" closable="false" text="FIND PATIENT">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                        <TextField fx:id="findinrecords" alignment="CENTER" layoutX="93.0"
                                                   layoutY="18.0" prefHeight="50.0" prefWidth="478.0"
                                                   promptText="SEARCH IN RECORDS">
                                            <font>
                                                <Font size="19.0"/>
                                            </font>
                                        </TextField>
                                        <Button fx:id="findinrecordsbutton" layoutX="582.0" layoutY="18.0"
                                                mnemonicParsing="false" prefHeight="50.0" prefWidth="117.0"
                                                text="SEARCH">
                                            <font>
                                                <Font size="19.0"/>
                                            </font>
                                        </Button>
                                        <AnchorPane layoutX="71.0" layoutY="85.0" prefHeight="433.0" prefWidth="962.0"
                                                    style="-fx-background-color: white;" styleClass="innerform">
                                            <children>
                                                <ScrollPane layoutX="8.0" layoutY="13.0" prefHeight="408.0"
                                                            prefWidth="942.0">
                                                    <content>
                                                        <AnchorPane prefHeight="406.0" prefWidth="925.0">
                                                            <children>
                                                                <TableView fx:id="patienttable" layoutX="-2.0"
                                                                           layoutY="2.0" prefHeight="388.0"
                                                                           prefWidth="926.0">
                                                                    <columns>
                                                                        <TableColumn fx:id="colpatientname"
                                                                                     prefWidth="295.20005798339844"
                                                                                     text="NAME"/>
                                                                        <TableColumn fx:id="colpatientemail"
                                                                                     prefWidth="295.20005798339844"
                                                                                     text="EMAIL"/>
                                                                        <TableColumn fx:id="colpatientnumber"
                                                                                     prefWidth="333.0"
                                                                                     text="PHONE NUMBER"/>
                                                                    </columns>
                                                                </TableView>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </ScrollPane>
                                            </children>
                                        </AnchorPane>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab fx:id="addpatienthistoryTab" closable="false" text="PATIENT RECORDS">
                            <content>
                                <AnchorPane fx:id="addPatientHistoryContainer" minHeight="0.0" minWidth="0.0"
                                            prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                        <TabPane fx:id="tabcontainerhistorypane" layoutX="-2.0" layoutY="1.0"
                                                 prefHeight="602.0" prefWidth="1200.0" tabClosingPolicy="UNAVAILABLE">
                                            <tabs>
                                                <Tab text="ADD CONDITIONS">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0"/>
                                                    </content>
                                                </Tab>
                                                <Tab text="VIEW HISTORY">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0"/>
                                                    </content>
                                                </Tab>
                                                <Tab text="CONDITIONS">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0"/>
                                                    </content>
                                                </Tab>
                                            </tabs>
                                        </TabPane>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab fx:id="labtestsTab" closable="false" text="CLINIC">
                            <content>
                                <AnchorPane fx:id="labtestscontainer" minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                            prefWidth="200.0">
                                    <children>
                                        <TabPane fx:id="tabcontainerclinicpane" layoutX="3.0" prefHeight="604.0"
                                                 prefWidth="1200.0" tabClosingPolicy="UNAVAILABLE">
                                            <tabs>
                                                <Tab text="APPOINTMENTS">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0"/>
                                                    </content>
                                                </Tab>
                                                <Tab text="LAB TESTS">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0"/>
                                                    </content>
                                                </Tab>
                                                <Tab text="DIAGNOSIS">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0"/>
                                                    </content>
                                                </Tab>
                                                <Tab text="PRESCRIPTION">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0"/>
                                                    </content>
                                                </Tab>
                                            </tabs>
                                        </TabPane>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </StackPane>
        <Label fx:id="clock" alignment="CENTER" layoutX="10.0" layoutY="9.0" prefHeight="49.0" prefWidth="327.0"
               text="Label">
            <font>
                <Font size="26.0"/>
            </font>
        </Label>
        <Button fx:id="logout" layoutX="978.0" layoutY="9.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="171.0"
                text="LOG OUT">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <Label fx:id="title" alignment="CENTER" layoutX="384.0" layoutY="9.0" prefHeight="49.0" prefWidth="502.0"
               text="Label">
            <font>
                <Font size="26.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
