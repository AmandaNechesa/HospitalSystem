<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="panel" prefHeight="700.0" prefWidth="1200.0" styleClass="basicscene"
            stylesheets="@../css/basicscene.css" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Receptionist.Panel">
    <AnchorPane layoutX="31.0" minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="1139.0">
        <children>
            <Label fx:id="clock" alignment="CENTER" layoutX="69.0" layoutY="25.0" prefHeight="50.0" prefWidth="348.0"
                   text="Label">
                <font>
                    <Font size="19.0"/>
                </font>
            </Label>
            <Button fx:id="logout" layoutX="570.0" layoutY="20.0" mnemonicParsing="false" prefHeight="60.0"
                    prefWidth="163.0" text="LOG OUT">
                <font>
                    <Font size="19.0"/>
                </font>
            </Button>
            <TabPane layoutX="24.0" layoutY="92.0" prefHeight="604.0" prefWidth="1108.0" tabClosingPolicy="UNAVAILABLE">
                <tabs>
                    <Tab text="EXISTING PATIENTS">
                        <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                <children>
                                    <TextField alignment="CENTER" layoutX="93.0" layoutY="18.0" prefHeight="50.0"
                                               prefWidth="306.0" promptText="SEARCH IN RECORDS">
                                        <font>
                                            <Font size="19.0"/>
                                        </font>
                                    </TextField>
                                    <Button layoutX="423.0" layoutY="15.0" mnemonicParsing="false" prefHeight="50.0"
                                            prefWidth="117.0" text="SEARCH">
                                        <font>
                                            <Font size="19.0"/>
                                        </font>
                                    </Button>
                                </children>
                            </AnchorPane>
                        </content>
                    </Tab>
                    <Tab text="NEW PATIENTS">
                        <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="550.0" prefWidth="1108.0">
                                <children>
                                    <AnchorPane layoutX="206.0" layoutY="25.0" prefHeight="513.0" prefWidth="767.0"
                                                style="-fx-background-color: white;" styleClass="innerform">
                                        <children>
                                            <TextField fx:id="patientname" alignment="CENTER" layoutX="27.0"
                                                       layoutY="14.0" prefHeight="50.0" prefWidth="348.0"
                                                       promptText="NAME">
                                                <font>
                                                    <Font size="19.0"/>
                                                </font>
                                            </TextField>
                                            <TextField fx:id="patientemail" alignment="CENTER" layoutX="27.0"
                                                       layoutY="71.0" prefHeight="50.0" prefWidth="348.0"
                                                       promptText="EMAIL">
                                                <font>
                                                    <Font size="19.0"/>
                                                </font>
                                            </TextField>
                                            <TextField alignment="CENTER" layoutX="27.0" layoutY="132.0"
                                                       prefHeight="50.0" prefWidth="348.0" fx:id="phonenumber"
                                                       promptText="PHONE NUMBER">
                                                <font>
                                                    <Font size="19.0"/>
                                                </font>
                                            </TextField>
                                            <Button fx:id="addpatient" layoutX="236.0" layoutY="401.0"
                                                    mnemonicParsing="false" prefHeight="79.0" prefWidth="296.0"
                                                    text="ADD PATIENT">
                                                <font>
                                                    <Font size="21.0"/>
                                                </font>
                                            </Button>
                                            <RadioButton layoutX="428.0" layoutY="22.0" mnemonicParsing="false"
                                                         prefHeight="34.0" prefWidth="122.0" text="MALE"/>
                                            <RadioButton layoutX="428.0" layoutY="71.0" mnemonicParsing="false"
                                                         prefHeight="34.0" prefWidth="122.0" text="FEMALE"/>
                                            <DatePicker fx:id="dob" layoutX="27.0" layoutY="211.0" prefHeight="44.0"
                                                        prefWidth="348.0" promptText="DOB" styleClass="inputefield"/>
                                            <Label fx:id="datepicked" alignment="CENTER" layoutX="391.0" layoutY="211.0"
                                                   prefHeight="50.0" prefWidth="283.0" text="Label">
                                                <font>
                                                    <Font size="19.0"/>
                                                </font>
                                            </Label>
                                        </children>
                                    </AnchorPane>
                                </children>
                            </AnchorPane>
                        </content>
                    </Tab>
                </tabs>
            </TabPane>
        </children>
    </AnchorPane>

</AnchorPane>
